{"type":"JOB_SUBMITTED","event":{"org.apache.hadoop.mapreduce.jobhistory.JobSubmitted":{"jobid":"job_1506281836527_0056","jobName":"select  i_item_desc \n      ,i_category...100(Stage-4)","userName":"ubuntu","submitTime":1506437800048,"jobConfPath":"hdfs://10.254.0.136:8020/tmp/hadoop-yarn/staging/ubuntu/.staging/job_1506281836527_0056/job.xml","acls":{},"jobQueueName":"default","workflowId":{"string":"hive_ubuntu_20170926095438_cb471ccd-b8af-45b7-b9eb-8d1a268a47fe"},"workflowName":{"string":"select  i_item_desc \n      ,i_category \n      ,i_class \n      ,i_current_price\n      ,i_item_id\n      ,sum(ws_ext_sales_price) as itemrevenue \n      ,sum(ws_ext_sales_price)*100/sum(sum(ws_ext_sales_price)) over\n          (partition by i_class) as revenueratio\nfrom\t\n\tweb_sales\n    \t,item \n    \t,date_dim\nwhere \n\tweb_sales.ws_item_sk = item.i_item_sk \n  \tand item.i_category in ('Jewelry', 'Sports', 'Books')\n  \tand web_sales.ws_sold_date_sk = date_dim.d_date_sk\n\tand date_dim.d_date between '2001-01-12' and '2001-02-11'\ngroup by \n\ti_item_id\n        ,i_item_desc \n        ,i_category\n        ,i_class\n        ,i_current_price\norder by \n\ti_category\n        ,i_class\n        ,i_item_id\n        ,i_item_desc\n        ,revenueratio\nlimit 100"},"workflowNodeName":{"string":"Stage-4"},"workflowAdjacencies":{"string":"\"Stage-8\"=\"Stage-3\" \"Stage-9\"=\"Stage-3\" \"Stage-10\"=\"Stage-14,Stage-15,Stage-2\" \"Stage-2\"=\"Stage-3\" \"Stage-11\"=\"Stage-10\" \"Stage-3\"=\"Stage-4\" \"Stage-13\"=\"Stage-16,Stage-1\" \"Stage-1\"=\"Stage-10\" \"Stage-14\"=\"Stage-8\" \"Stage-15\"=\"Stage-9\" \"Stage-16\"=\"Stage-11\" \"Stage-4\"=\"Stage-5\" "},"workflowTags":{"string":""}}}}

