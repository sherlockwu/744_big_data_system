{"type":"JOB_SUBMITTED","event":{"org.apache.hadoop.mapreduce.jobhistory.JobSubmitted":{"jobid":"job_1506281836527_0079","jobName":"select i_brand_id brand_id, i_b...i_brand_id(Stage-5)","userName":"ubuntu","submitTime":1506439455850,"jobConfPath":"hdfs://10.254.0.136:8020/tmp/hadoop-yarn/staging/ubuntu/.staging/job_1506281836527_0079/job.xml","acls":{},"jobQueueName":"default","workflowId":{"string":"hive_ubuntu_20170926101939_22aaa33e-779d-4d09-9538-bfcd09abd32c"},"workflowName":{"string":"select i_brand_id brand_id, i_brand brand,t_hour,t_minute,\n \tsum(ext_price) ext_price\n from item JOIN (select ws_ext_sales_price as ext_price, \n                        ws_sold_date_sk as sold_date_sk,\n                        ws_item_sk as sold_item_sk,\n                        ws_sold_time_sk as time_sk  \n                 from web_sales,date_dim\n                 where date_dim.d_date_sk = web_sales.ws_sold_date_sk\n                   and d_moy=12\n                   and d_year=2001\n                 union all\n                 select cs_ext_sales_price as ext_price,\n                        cs_sold_date_sk as sold_date_sk,\n                        cs_item_sk as sold_item_sk,\n                        cs_sold_time_sk as time_sk\n                 from catalog_sales,date_dim\n                 where date_dim.d_date_sk = catalog_sales.cs_sold_date_sk\n                   and d_moy=12\n                   and d_year=2001\n                 union all\n                 select ss_ext_sales_price as ext_price,\n                        ss_sold_date_sk as sold_date_sk,\n                        ss_item_sk as sold_item_sk,\n                        ss_sold_time_sk as time_sk\n                 from store_sales,date_dim\n                 where date_dim.d_date_sk = store_sales.ss_sold_date_sk\n                   and d_moy=12\n                   and d_year=2001\n                 ) tmp ON tmp.sold_item_sk = item.i_item_sk\n JOIN time_dim ON tmp.time_sk = time_dim.t_time_sk\n where\n       i_manager_id=1\n   and (t_meal_time = 'breakfast' or t_meal_time = 'dinner')\n group by i_brand, i_brand_id,t_hour,t_minute\n order by ext_price desc, i_brand_id\n "},"workflowNodeName":{"string":"Stage-5"},"workflowAdjacencies":{"string":"\"Stage-19\"=\"Stage-29,Stage-1\" \"Stage-2\"=\"Stage-26\" \"Stage-1\"=\"Stage-16\" \"Stage-7\"=\"Stage-16\" \"Stage-4\"=\"Stage-5\" \"Stage-9\"=\"Stage-16\" \"Stage-30\"=\"Stage-20\" \"Stage-31\"=\"Stage-23\" \"Stage-20\"=\"Stage-16\" \"Stage-22\"=\"Stage-30,Stage-7\" \"Stage-23\"=\"Stage-16\" \"Stage-25\"=\"Stage-31,Stage-9\" \"Stage-14\"=\"Stage-26\" \"Stage-26\"=\"Stage-4\" \"Stage-15\"=\"Stage-26\" \"Stage-27\"=\"Stage-14\" \"Stage-16\"=\"Stage-27,Stage-28,Stage-2\" \"Stage-28\"=\"Stage-15\" \"Stage-17\"=\"Stage-16\" \"Stage-29\"=\"Stage-17\" "},"workflowTags":{"string":""}}}}

