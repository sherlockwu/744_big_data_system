{"type":"JOB_SUBMITTED","event":{"org.apache.hadoop.mapreduce.jobhistory.JobSubmitted":{"jobid":"job_1506281836527_0064","jobName":"select  *\n from(select w_warehouse_nam...100(Stage-12)","userName":"ubuntu","submitTime":1506438473036,"jobConfPath":"hdfs://10.254.0.136:8020/tmp/hadoop-yarn/staging/ubuntu/.staging/job_1506281836527_0064/job.xml","acls":{},"jobQueueName":"default","workflowId":{"string":"hive_ubuntu_20170926100618_2a21b8e3-ae97-4e83-a5b8-9c6a2f486375"},"workflowName":{"string":"select  *\n from(select w_warehouse_name\n            ,i_item_id\n            ,sum(case when (cast(d_date as date) < cast ('1998-04-08' as date))\n\t                then inv_quantity_on_hand \n                      else 0 end) as inv_before\n            ,sum(case when (cast(d_date as date) >= cast ('1998-04-08' as date))\n                      then inv_quantity_on_hand \n                      else 0 end) as inv_after\n   from inventory\n       ,warehouse\n       ,item\n       ,date_dim\n   where i_current_price between 0.99 and 1.49\n     and item.i_item_sk          = inventory.inv_item_sk\n     and inventory.inv_warehouse_sk   = warehouse.w_warehouse_sk\n     and inventory.inv_date_sk    = date_dim.d_date_sk\n     and d_date between '1998-03-09' and '1998-05-07'\n   group by w_warehouse_name, i_item_id) x\n where (case when inv_before > 0 \n             then inv_after / inv_before \n             else null\n             end) between 2.0/3.0 and 3.0/2.0\n order by w_warehouse_name\n         ,i_item_id\n limit 100"},"workflowNodeName":{"string":"Stage-12"},"workflowAdjacencies":{"string":"\"Stage-19\"=\"Stage-13\" \"Stage-2\"=\"Stage-11\" \"Stage-3\"=\"Stage-4\" \"Stage-4\"=\"Stage-5\" \"Stage-9\"=\"Stage-4\" \"Stage-20\"=\"Stage-15\" \"Stage-10\"=\"Stage-4\" \"Stage-11\"=\"Stage-16,Stage-17,Stage-3\" \"Stage-12\"=\"Stage-11\" \"Stage-13\"=\"Stage-11\" \"Stage-14\"=\"Stage-18,Stage-19,Stage-2\" \"Stage-15\"=\"Stage-14\" \"Stage-16\"=\"Stage-9\" \"Stage-17\"=\"Stage-10\" \"Stage-18\"=\"Stage-12\" "},"workflowTags":{"string":""}}}}

